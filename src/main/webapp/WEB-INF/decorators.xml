<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">  
    <!-- 此处用来定义不需要过滤的页面 -->  
    <excludes>
    	<pattern>/css/*</pattern>
    	<pattern>/js/*</pattern>
    	<pattern>/manage/login.jsp</pattern>
    </excludes>  
  
 	<!-- 用来定义装饰器要过滤的页面 -->  
    <decorator name="template-manage" page="template-manage.jsp">  
        <pattern>/manage/*</pattern>  
    </decorator>  
</decorators>